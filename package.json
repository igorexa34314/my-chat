{
	"name": "vue-chat-2023",
	"description": "My first chat based on Vue 3 and Firebase SDK",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build && cp dist/index.html dist/404.html",
		"build:analyze": "export ROLLUP_ANALYZE=true || set ROLLUP_ANALYZE=true && vue-tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write --cache src/"
	},
	"dependencies": {
		"@imengyu/vue3-context-menu": "^1.3.3",
		"@mdi/js": "^7.2.96",
		"@vueuse/core": "^10.4.1",
		"algoliasearch": "^4.20.0",
		"base64-arraybuffer": "^1.0.2",
		"firebase": "^10.3.1",
		"gsap": "^3.12.2",
		"pinia": "^2.1.6",
		"uuid": "^9.0.1",
		"vue": "^3.3.4",
		"vue-i18n": "^9.4.0",
		"vue-instantsearch": "^4.10.10",
		"vue-meta": "3.0.0-alpha.10",
		"vue-router": "^4.2.4",
		"vuetify": "^3.3.15"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^0.12.3",
		"@rushstack/eslint-patch": "^1.3.3",
		"@types/node": "^20.6.0",
		"@types/uuid": "^9.0.3",
		"@vitejs/plugin-vue": "^4.3.4",
		"@vue/eslint-config-prettier": "^8.0.0",
		"@vue/eslint-config-typescript": "^11.0.3",
		"eslint": "^8.49.0",
		"eslint-plugin-vue": "^9.17.0",
		"prettier": "^3.0.3",
		"rollup-plugin-visualizer": "^5.9.2",
		"sass": "^1.66.1",
		"typescript": "^5.2.2",
		"unplugin-vue-router": "^0.6.4",
		"vite": "^4.4.9",
		"vite-plugin-vue-layouts": "^0.8.0",
		"vite-plugin-vuetify": "^1.0.2",
		"vue-tsc": "^1.8.10"
	},
	"packageManager": "pnpm@8.6.12",
	"repository": {
		"type": "git",
		"url": "https://github.com/igorexa34314/vue-chat-2023.git"
	},
	"eslintIgnore": [
		"./functions/**"
	]
}
