{
	"name": "vue-chat-2023",
	"description": "My first chat based on Vue 3 and Firebase SDK",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" -- && cp dist/index.html dist/404.html",
		"preview": "vite preview",
		"build-only": "vite build",
		"build:analyze": "export ROLLUP_ANALYZE=true || set ROLLUP_ANALYZE=true && run-s type-check build",
		"type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
		"lint": "eslint . --no-eslintrc --config .eslintrc.cjs --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
		"format": "prettier --write --cache src/"
	},
	"dependencies": {
		"@imengyu/vue3-context-menu": "^1.3.3",
		"@mdi/js": "^7.3.67",
		"@vueuse/core": "^10.7.0",
		"algoliasearch": "^4.20.0",
		"base64-arraybuffer": "^1.0.2",
		"firebase": "^10.7.1",
		"gsap": "^3.12.3",
		"pinia": "^2.1.7",
		"uuid": "^9.0.1",
		"vue": "^3.3.10",
		"vue-i18n": "^9.8.0",
		"vue-instantsearch": "^4.13.1",
		"vue-meta": "3.0.0-alpha.10",
		"vue-router": "^4.2.5",
		"vuetify": "^3.4.6"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^1.5.0",
		"@rushstack/eslint-patch": "^1.6.0",
		"@tsconfig/node18": "^18.2.2",
		"@types/node": "^20.10.4",
		"@types/uuid": "^9.0.7",
		"@vitejs/plugin-vue": "^4.5.2",
		"@vue/eslint-config-prettier": "^8.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/tsconfig": "^0.4.0",
		"eslint": "^8.55.0",
		"eslint-plugin-vue": "^9.19.2",
		"npm-run-all2": "^6.1.1",
		"prettier": "^3.1.0",
		"rollup-plugin-visualizer": "^5.10.0",
		"sass": "^1.69.5",
		"typescript": "^5.3.3",
		"unplugin-vue-router": "^0.7.0",
		"vite": "^4.5.1",
		"vite-plugin-vue-layouts": "^0.8.0",
		"vite-plugin-vuetify": "^1.0.2",
		"vue-tsc": "^1.8.25"
	},
	"packageManager": "pnpm@8.6.12",
	"repository": {
		"type": "git",
		"url": "https://github.com/igorexa34314/vue-chat-2023.git"
	}
}
